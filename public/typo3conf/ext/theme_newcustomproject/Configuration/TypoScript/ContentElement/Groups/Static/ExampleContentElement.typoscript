# ###########################
# exampleContentElement
#
# CType: exampleContentElement
# ===========================
tt_content {
  exampleContentElement =< lib.contentElement
  exampleContentElement {

    templateName = exampleContentElement

    dataProcessing {
      10 = TYPO3\CMS\Frontend\DataProcessing\FlexFormProcessor
      10 {
        fieldName = pi_flexform
        as = flexform
      }
      20 = TYPO3\CMS\Frontend\DataProcessing\FilesProcessor
      20 {
        references.fieldName = picture
      }
      30 = TYPO3\CMS\Frontend\DataProcessing\FilesProcessor
      30 {
        references.fieldName = svg_image
        as = svg_image
      }
    }
  }
}
