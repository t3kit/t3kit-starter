{
  "name": "t3kit/t3kit-starter",
  "description": "Starter point for t3kit project. t3kit and TYPO3 dependencies, folder structure, custom site configurations, local environment based on Docker, CI/CD configs",
  "type": "project",
  "keywords": [
    "t3kit",
    "typo3",
    "cms",
    "starter",
    "t3kit9"
  ],
  "homepage": "http://t3kit.com",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "t3kit Community",
      "homepage": "https://github.com/t3kit"
    }
  ],
  "support": {
    "issues": "https://github.com/t3kit/t3kit-starter/issues",
    "forum": "https://typo3.slack.com/messages/t3kit",
    "source": "https://github.com/t3kit/t3kit-starter",
    "docs": "https://github.com/t3kit/t3kit-starter"
  },
  "require": {
    "typo3/cms-backend": "dev-master",
    "typo3/cms-core": "dev-master",
    "typo3/cms-extbase": "dev-master",
    "typo3/cms-extensionmanager": "dev-master",
    "typo3/cms-filelist": "dev-master",
    "typo3/cms-fluid": "dev-master",
    "typo3/cms-frontend": "dev-master",
    "typo3/cms-install": "dev-master",
    "typo3/cms-recordlist": "dev-master",

    "typo3/cms-about": "dev-master",
    "typo3/cms-adminpanel": "dev-master",
    "typo3/cms-belog": "dev-master",
    "typo3/cms-beuser": "dev-master",
    "typo3/cms-form": "dev-master",
    "typo3/cms-impexp": "dev-master",
    "typo3/cms-info": "dev-master",
    "typo3/cms-lowlevel": "dev-master",
    "typo3/cms-recycler": "dev-master",
    "typo3/cms-redirects": "dev-master",
    "typo3/cms-reports": "dev-master",
    "typo3/cms-rte-ckeditor": "dev-master",
    "typo3/cms-scheduler": "dev-master",
    "typo3/cms-seo": "dev-master",
    "typo3/cms-setup": "dev-master",
    "typo3/cms-tstemplate": "dev-master",
    "typo3/cms-viewpage": "dev-master",

    "typo3/cms-t3editor": "dev-master",
    "typo3/cms-felogin": "dev-master",
    "typo3/cms-filemetadata": "dev-master",
    "typo3/cms-sys-note": "dev-master",

    "typo3/cms-indexed-search": "dev-master",
    "typo3/cms-linkvalidator": "dev-master",
    "typo3/cms-opendocs": "dev-master",

    "t3kit/t3kit": "dev-t3kit10"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4",
    "heahdude/yaml-linter": "^1.0"
  },
  "suggest": {},
  "autoload": {},
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.3"
    },
    "sort-packages": true
  },
  "scripts": {
    "typo3-cms-scripts": [
    ],
    "pre-install-cmd": [
      "composer validate --no-check-all --strict"
    ],
    "post-autoload-dump": [
    ],
    "post-create-project-cmd": [
      "composer env"
    ],
    "env": "cp .t3kit/docker/local.env .env",
    "dbup": "docker-compose exec -T web /var/www/html/.t3kit/db/setupdb.sh",
    "dbre": "docker-compose exec -T web /var/www/html/.t3kit/db/restoredb.sh",
    "dbp": "docker-compose exec -T web /var/www/html/.t3kit/db/packdb.sh",
    "lint-php": "vendor/bin/phpcs --colors",
    "lint-yaml": "vendor/bin/yaml-linter --ansi config/ .t3kit/",
    "lint": [
      "@lint-php",
      "@lint-yaml"
    ]
  }
}
