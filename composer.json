{
	"name": "t3kit/t3kit-starter",
	"description": "Starter point for t3kit project. t3kit and TYPO3 dependencies, folder structure, custom site configurations, local environment based on Docker, CI/CD configs",
	"type": "project",
	"keywords": [
		"t3kit",
		"typo3",
		"cms",
		"starter",
		"t3kit9"
	],
	"homepage": "http://t3kit.com",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "t3kit Community",
			"homepage": "https://github.com/t3kit"
		}
	],
	"support": {
		"issues": "https://github.com/t3kit/t3kit-starter/issues",
		"forum": "https://typo3.slack.com/messages/t3kit",
		"source": "https://github.com/t3kit/t3kit-starter",
		"docs": "https://github.com/t3kit/t3kit-starter"
	},
	"require": {
		"typo3/minimal": "^9.5",

        "typo3/cms-about": "^9.5",
        "typo3/cms-adminpanel": "^9.5",
        "typo3/cms-belog": "^9.5",
        "typo3/cms-beuser": "^9.5",
        "typo3/cms-fluid-styled-content": "^9.5",
        "typo3/cms-form": "^9.5",
        "typo3/cms-impexp": "^9.5",
        "typo3/cms-info": "^9.5",
        "typo3/cms-lowlevel": "^9.5",
        "typo3/cms-recycler": "^9.5",
        "typo3/cms-redirects": "^9.5",
        "typo3/cms-reports": "^9.5",
        "typo3/cms-rte-ckeditor": "^9.5",
        "typo3/cms-scheduler": "^9.5",
        "typo3/cms-seo": "^9.5",
        "typo3/cms-setup": "^9.5",
        "typo3/cms-tstemplate": "^9.5",
        "typo3/cms-viewpage": "^9.5",

        "typo3/cms-t3editor": "^9.5",
        "typo3/cms-felogin": "^9.5",
        "typo3/cms-filemetadata": "^9.5",
        "typo3/cms-sys-note": "^9.5",

        "typo3/cms-feedit": "^9.5",
        "typo3/cms-indexed-search": "^9.5",
        "typo3/cms-linkvalidator": "^9.5",
        "typo3/cms-opendocs": "^9.5",
        "typo3/cms-sys-action": "^9.5",
        "typo3/cms-taskcenter": "^9.5",

		"helhum/typo3-console": "^5.5.5",

		"t3kit/t3kit": "dev-t3kit9"
	},
	"require-dev": {},
	"suggest": {},
	"prefer-stable": true,
	"config": {
		"platform": {
			"php": "7.2"
		}
	},
	"scripts": {
		"typo3-cms-scripts": [
			"typo3cms install:generatepackagestates",
			"typo3cms install:fixfolderstructure"
		],
		"pre-install-cmd": [
			"composer validate --no-check-all --strict"
		],
		"post-autoload-dump": [
			"@typo3-cms-scripts"
		],
		"env": "cp .t3kit/docker/.env.example .env",
		"dbup": "docker-compose exec -T web /var/www/html/.t3kit/db/setupdb.sh",
		"dbre": "docker-compose exec -T web /var/www/html/.t3kit/db/restoredb.sh",
		"dbp": "docker-compose exec -T web /var/www/html/.t3kit/db/packdb.sh"
	}
}
